{
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^pages/(.*)$", "^components/(.*)$", "^[./]" ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
