{
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "trailingComma": "all",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^react$", "<THIRD_PARTY_MODULES>", "^\\.\/pages/(.*)$", "^\\.\/components/(.*)$", "^[./]" ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
